{
    "name": "hannanyusop/toyyibpay",
    "description": "Laravel 9 Package for ToyyibPay API (UPDATED)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ABDUL",
            "email": "noreply@mail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hannan\\Toyyibpay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hannan\\Toyyibpay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hannan\\Toyyibpay\\ToyyibpayServiceProvider"
            ],
            "aliases": {
                "Toyyibpay": "Hannan\\Toyyibpay\\ToyyibpayFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.3|^7.0.1",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "orchestra/testbench": "^6.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    }
}
